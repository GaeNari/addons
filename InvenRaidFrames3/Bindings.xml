	</Binding>
	<Binding name="CLICK InvenRaidFrames3Overlord:LeftButton" category="ADDONS"/>
	<Binding name="INVENRAIDFRAMES3_DOREADYCHECK" category="ADDONS">
		if InvenRaidFrames3:IsLeader() then
			DoReadyCheck()
		end
	</Binding>
	<Binding name="INVENRAIDFRAMES3_INITIATEROLEPOLL" category="ADDONS">
